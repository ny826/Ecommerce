<h4> Login Here</h4>

<div class="login">
<br>
<%= form_tag '/signin' do %>

<%= text_field_tag :email, @email,  :size =>"20x2", :placeholder => 'Enter email' %><br><br>
<%= password_field_tag :password,@password, :size =>"20x2",:placeholder => 'Enter Password' %><br><br>
<%= submit_tag "Login"%><br><br>
<a href="/signup">New user>></a>

<%end%>
</div>


